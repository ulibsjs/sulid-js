{
  "name": "sulid-js",
  "version": "0.0.1",
  "description": "Solid-js ssr framework similar to NextJS",
  "scripts": {
    "dev": "ts-node server/index.ts",
    "start": "ts-node server/index.ts prod",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr server/entry.tsx",
    "build": "npm run build:client && npm run build:server"
  },
  "author": "untlsn",
  "license": "MIT",
  "dependencies": {
    "directory-tree": "^3.0.1",
    "express": "^4.17.2",
    "solid-app-router": "^0.2.1",
    "ts-node": "^10.4.0",
    "vite": "^2.7.12",
    "vite-plugin-solid": "^2.2.2"
  },
  "peerDependencies": {
    "solid-js": "^1.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "solid-js": "^1.3.3",
    "typescript": "^4.5.4"
  }
}
